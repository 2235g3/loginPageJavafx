<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox alignment="CENTER" prefHeight="174.0" prefWidth="404.0" spacing="20.0" xmlns="http://javafx.com/javafx/" xmlns:fx="http://javafx.com/fxml/" fx:controller="com.company.loginpagejavafx.mainController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>

    <Label alignment="TOP_LEFT" text="Choose to either login or signup:" textAlignment="CENTER" />
    <ButtonBar prefHeight="73.0" prefWidth="364.0">
        <buttons>
            <Button fx:id="signUp" onAction="#signUpPage" prefHeight="42.0" prefWidth="227.0" text="Sign Up" />
            <Button fx:id="logIn" onAction="#logInPage" prefHeight="41.0" text="Log In" />
        </buttons>
    </ButtonBar>
</VBox>