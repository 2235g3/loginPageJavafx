<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="236.0" prefWidth="426.0" xmlns="http://javafx.com/javafx/11.0.2" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.company.loginpagejavafx.mainController">
    <children>
        <TextField fx:id="usernameInput" layoutX="123.0" layoutY="67.0" prefHeight="25.0" prefWidth="175.0" promptText="Username" />
        <PasswordField fx:id="passwordInput" layoutX="123.0" layoutY="118.0" prefHeight="25.0" prefWidth="175.0" promptText="Password" />
        <TextField fx:id="passwordShownInput" layoutX="123.0" layoutY="118.0" prefHeight="25.0" prefWidth="175.0" promptText="Password" visible="false" />
        <CheckBox fx:id="showPassCheck" layoutX="306.0" layoutY="122.0" mnemonicParsing="false" onAction="#showHidePass" text="Show Password" />
        <Button fx:id="backButton" layoutX="14.0" layoutY="190.0" mnemonicParsing="false" onAction="#backToChoose" text="Back" />
        <Button fx:id="signUpButton" layoutX="182.0" layoutY="165.0" mnemonicParsing="false" onAction="#signUp" text="Sign Up" />
        <Label layoutX="35.0" layoutY="120.0" prefHeight="21.0" prefWidth="73.0" text="Password:">
            <font>
                <Font size="15.0" />
            </font>
        </Label>
        <Label layoutX="35.0" layoutY="69.0" text="Username:">
            <font>
                <Font size="15.0" />
            </font>
        </Label>
        <Label fx:id="errorText" layoutX="154.0" layoutY="207.0" text="An error occurred!" textFill="RED" visible="false" />
      <Label layoutX="170.0" layoutY="14.0" text="Sign Up" underline="true">
         <font>
            <Font name="System Bold" size="22.0" />
         </font>
      </Label>
    </children>
</AnchorPane>
